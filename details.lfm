        Font.Style = [fsBold]
        ParentColor = False
        ParentFont = False
        WordWrap = True
      end
      object LabelAzonDuoTrans: TLabel
        Left = 10
        Height = 30
        Top = 35
        Width = 888
        Align = alTop
        BorderSpacing.Around = 10
        Caption = 'Аллоҳумма! Робба ҳазиҳид-даъватит таммаҳ. Вассолатил қоимаҳ, ати Муҳаммаданил васийлата вал фазийлаҳ. Вабъасҳу мақомам маҳмуданиллазий ваъадтаҳ. Варзуқна шафаъатаҳу явмал қиёмаҳ. Иннака лаа тухлифул мийъад.'
        Font.Style = [fsBold]
        ParentColor = False
        ParentFont = False
        WordWrap = True
      end
      object LabelAzonDuoUz: TLabel
        Left = 10
        Height = 60
        Top = 75
        Width = 888
        Align = alTop
        BorderSpacing.Around = 10
        Caption = 'Маъноси:'#13#10'Эй ушбу мукаммал дуо ва ҳамда абадий салавот Парвардигори. Муҳаммад алайҳиссаломнинг мартабаларини улуғ қилиб, неъматларингни унга бисёр қилгайсан, ул зотга Ўзинг ваъда қилган макоми маҳмудни (жаннатдаги ўринни) ато этгайсан. Қиёматда бизга унинг шафоатини насиб этгайсан. Сен зинҳор ваъдага хилоф қилмайсан.'
        Font.Style = [fsItalic]
        ParentColor = False
